<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid
                android:color="#662045" />
            <stroke
                android:width="1dp"
                android:color="#662045" />
            <corners
                android:radius="1dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <solid
                android:color="#8f2b60" />
            <stroke
                android:width="1dp"
                android:color="#8f2b60" />
            <corners
                android:radius="1dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp" />
        </shape>
    </item>
</selector>